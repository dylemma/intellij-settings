<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_IMPORTS" value="false" />
  </component>
  <component name="CodeInsightSettings">
    <EXCLUDED_PACKAGE NAME="com.sun" />
    <EXCLUDED_PACKAGE NAME="reactive.Observing" />
    <EXCLUDED_PACKAGE NAME="sun" />
  </component>
  <component name="CodeVisionSettings">
    <option name="codeVisionGroupToPosition">
      <map>
        <entry key="inheritors" value="Default" />
        <entry key="problems" value="Default" />
        <entry key="references" value="Default" />
        <entry key="vcs.code.vision" value="Default" />
      </map>
    </option>
    <option name="disabledCodeVisionProviderIds">
      <set>
        <option value="inheritors" />
        <option value="problems" />
        <option value="references" />
        <option value="vcs.code.vision" />
      </set>
    </option>
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:PREVIEW" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc; *.json" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="SHOW_INTENTION_PREVIEW" value="true" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value=".inheritors" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="important" pattern="\b(todo|fixme)\b.*" />
    <pattern icon="important" pattern="TODO:.*" />
  </component>
</application>